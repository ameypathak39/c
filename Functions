#include <stdio.h>

// Function declaration
int add(int a, int b);
void printMessage();

int main() {
    int x = 5, y = 7;
    int sum = add(x, y); // Function call
    printf("Sum: %d\n", sum);

    printMessage(); // Function call

    return 0;
}

// Function definition
int add(int a, int b) {
    return a + b;
}

void printMessage() {
    printf("Hello, World!\n");
}
