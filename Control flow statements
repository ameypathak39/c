#include <stdio.h>

int main() {
    // If-Else statement
    int num = 10;
    if (num > 5) {
        printf("Number is greater than 5\n");
    } else {
        printf("Number is not greater than 5\n");
    }
    
    // Switch statement
    int choice = 2;
    switch (choice) {
        case 1:
            printf("Choice is 1\n");
            break;
        case 2:
            printf("Choice is 2\n");
            break;
        default:
            printf("Choice is not 1 or 2\n");
    }
    
    // For loop
    for (int i = 1; i <= 5; i++) {
        printf("Value of i: %d\n", i);
    }
    
    // While loop
    int count = 0;
    while (count < 3) {
        printf("Count: %d\n", count);
        count++;
    }
    
    // Do-While loop
    int x = 0;
    do {
        printf("Value of x: %d\n", x);
        x++;
    } while (x < 4);
    
    return 0;
}
